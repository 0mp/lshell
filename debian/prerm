#!/bin/sh
# postrm script for lshell
#
# $Id: prerm,v 1.3 2008-10-20 20:27:36 ghantoos Exp $
#

set -e

case "$1" in
    purge|remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
	# delete lshell group
	groupdel lshellg
    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

exit 0


